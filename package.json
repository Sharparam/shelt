{
  "name": "@sharparam/shelt",
  "version": "0.0.1",
  "description": "SvelteKit version of Sharparam's homepage",
  "keywords": [
    "sharparam",
    "homepage",
    "website",
    "svelte",
    "sveltekit"
  ],
  "homepage": "https://github.com/Sharparam/shelt",
  "bugs": {
    "url": "https://github.com/Sharparam/shelt/issues",
    "email": "shelt@sharparam.com"
  },
  "license": "MPL-2.0",
  "author": {
    "name": "Adam Hellberg",
    "email": "sharparam@sharparam.com",
    "url": "https://sharparam.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sharparam/shelt.git"
  },
  "private": true,
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "playwright test"
  },
  "type": "module",
  "devDependencies": {
    "@playwright/test": "next",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "date-fns": "2.28.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "mdsvex": "0.10.5",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "prism-themes": "1.9.0",
    "remark-emoji": "3.0.2",
    "remark-gfm": "3.0.1",
    "sanitize.css": "13.0.0",
    "sass": "1.51.0",
    "stylelint": "14.8.2",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "svelte": "3.48.0",
    "svelte-check": "2.7.0",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "packageManager": "yarn@3.2.0"
}
